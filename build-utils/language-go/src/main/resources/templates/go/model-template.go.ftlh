				m.${field.name?cap_first} = dt
					</#if>
				<#elseif helper.isArrayField(field)>
				var data []<#if helper.isComplexTypeReference(field.type)>*</#if>${helper.getLanguageTypeNameForField(field)}
				if err := d.DecodeElement(&data, &tok); err != nil {
