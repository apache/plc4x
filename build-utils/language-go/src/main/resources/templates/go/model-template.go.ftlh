						if err := d.DecodeElement(&dt, &tok); err != nil {
							return err
						}
						m.${field.name?cap_first} = append(m.${field.name?cap_first}, dt)
					default:
						break arrayLoop
					}
				}
					<#else>
				var dt *${helper.getLanguageTypeNameForField(field)}
				if err := d.DecodeElement(&dt, &tok); err != nil {
					if err == io.EOF {
						continue
					}
					return err
